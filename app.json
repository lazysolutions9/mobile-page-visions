{
  "expo": {
    "name": "Mobile Page Visions",
    "slug": "mobile-page-visions",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./public/logo1.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./public/logo1.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./public/logo1.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./public/favicon.ico"
    },
    "plugins": []
  }
} 